{
  "name": "graphql-findme-graphql-service",
  "version": "1.0.0",
  "description": "Service for connecting graphql to the mongoDB.",
  "scripts": {
    "prestart": "( ping -c 1 google.de && npm -s install ) || exit 0",
    "start": "nodemon lib/server.js --exec babel-node --presets es2015,stage-0",
    "build": "babel lib -d dist --presets es2015,stage-0",
    "lint": "eslint lib || exit 0",
    "serve": "node dist/server.js",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean && ( ping -c 1 google.de && npm -s install ) || exit 0",
    "preserve": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KSWE-2016-17/graphql-findme-graphql-service.git"
  },
  "author": "Alexej Esau, Ruben Gees",
  "license": "UNLICENCED",
  "bugs": {
    "url": "https://github.com/KSWE-2016-17/graphql-findme-graphql-service/issues"
  },
  "homepage": "https://github.com/KSWE-2016-17/graphql-findme-graphql-service#readme",
  "dependencies": {
    "body-parser": "1.15.2",
    "express": "4.13.3",
    "express-graphql": "0.5.4",
    "graphql": "^0.7.0",
    "graphql-compose": "^1.4.3",
    "graphql-compose-connection": "^2.1.1",
    "graphql-compose-mongoose": "^1.3.5",
    "graphql-date": "1.0.2",
    "mongoose": "^4.3.5"
  },
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-stage-0": "6.16.0",
    "eslint": "3.8.1",
    "nodemon": "1.11.0"
  }
}
